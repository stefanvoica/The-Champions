@model OnlineCleaningShop.Models.Category
<partial name="_Notifications" />



<h2>@ViewBag.Title</h2>

<br />

<p>@Model.CategoryName</p>


<form method="post" asp-action="Delete" asp-controller="Categories" asp-route-id="@Model.Id">

    <button class="btn btn-danger" type="submit">Sterge categoria</button>

</form>


<br />

<a class="btn alert-success" asp-action="Index" asp-controller="Categories">Afisare lista categorii</a>


<br />
<hr />
